// the plural forms
"%@ pageins, %@ pageouts" = "%1$@ gelesene Seiten, %2$@ geschriebene Seiten";
"%@ cache lookups, %@ cache hits (%@)" = "%1$@ Cache-Suchen, %2$@ Cache-Treffer (%3$@)";
"%@ page faults, %@ copy-on-writes" = "%1$@ Seitenfehler, %2$@ Kopien-nach-Schreiben";

